<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <bottom>
      <VBox fx:id="vbox" minHeight="-Infinity" prefHeight="400.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="labelEvents" text="Events" VBox.vgrow="NEVER">
               <font>
                  <Font name="Calibri" size="18.0" />
               </font>
            </Label>
            <TableView fx:id="events" prefHeight="0.0" prefWidth="1120.0" VBox.vgrow="ALWAYS">
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </VBox>
   </bottom>
   <center>
      <ScrollPane fx:id="charts" fitToHeight="true" prefHeight="270.0" prefViewportHeight="265.0" prefWidth="1120.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
